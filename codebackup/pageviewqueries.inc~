<?php

function getpageviewsurl($page,$month,$language)
{
	$page = str_replace(" ","_",$page);
	return "http://stats.grok.se/" . $language . "/" . $month . "/" . $page;
}

function getpageurl($page,$language)
{
	$page = str_replace(" ","_",$page);
	return "http://" . $language . ".wikipedia.org/wiki/" . $page;
}

?>